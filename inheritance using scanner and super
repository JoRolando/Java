import java.util.*;
 class animal{
 		String name;
 		animal (String name){
 			this.name=name ;
 			System.out.println(name + "is an animal");
 		}
 		void eat(){
 			System.out.println(name+"is eating");
 		}
}

class buff extends animal{
	buff(){
		super("tentails  ");
	}
	 void speak(){
		System.out.println(name+"says maaaayyyyy");
	}
}  
 
 public class inheritance_{
 	public static void main(String[] args)
 	{
 		buff an= new buff();
 		an.eat();
 		an.speak();	
 	}
 }
